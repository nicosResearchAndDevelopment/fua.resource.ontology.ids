ids3c-co:TDS_NDR_1_13_1_ExplicitAccessRequestApproval_TrustSecurityProfile a ids3cm:ComponentTestDesignSpecification_TrustSecurityProfile ;
    rdfs:isDefinedBy ids3c-co:IDSA ;
    rdfs:label "Trust Security Profile"@en ;
    dct:description "Definition of the approach to be taken for criterion <ids3c-co:NDR_1_13_1> 'Explicit access request approval' in case of an evaluation for the 'Trust Security Profile'."@en ;
