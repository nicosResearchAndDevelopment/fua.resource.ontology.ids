    ids3cm:criteria ids3c-co:CR_3_5 ;
    ids3cm:testDesignApplicability "This criterion is only applicable for connectors used in an Industrial context.\r\n                                     It affects any components accepting input from users or external interfaces."@en ;
    ids3cm:testDesignSecurityProfile ids3c-co:TDS_CR_3_5_InputValidation_BaseSecurityProfile, ids3c-co:TDS_CR_3_5_InputValidation_TrustSecurityProfile, ids3c-co:TDS_CR_3_5_InputValidation_TrustPlusSecurityProfile .

_:n3-441 a ids3cm:InitialCondition ;
    dct:description "The system integrity contains input validation that ensures the syntax, length and content for every input that impacts directly the action of the application or device."@en .

_:n3-442 a ids3cm:FinalCondition ;
