    ids3cm:testDesignSecurityProfile ids3c-co:TDS_CR_1_9_StrengthOfPublicKeybasedAuthentication_BaseSecurityProfile, ids3c-co:TDS_CR_1_9_StrengthOfPublicKeybasedAuthentication_TrustSecurityProfile, ids3c-co:TDS_CR_1_9_StrengthOfPublicKeybasedAuthentication_TrustPlusSecurityProfile .

