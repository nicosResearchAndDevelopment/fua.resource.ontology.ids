    ids3cm:testDesignSecurityProfile ids3c-co:TDS_DEV_S_FR_3_StatusOfCorrectiveMeasures_BaseSecurityProfile, ids3c-co:TDS_DEV_S_FR_3_StatusOfCorrectiveMeasures_TrustSecurityProfile, ids3c-co:TDS_DEV_S_FR_3_StatusOfCorrectiveMeasures_TrustPlusSecurityProfile .

_:n3-431 a ids3cm:InitialCondition ;
    dct:description "The SUT's delivery documentation must contain the flaw remediation procedures to identify the different stages of security flaws.\r\n                                                This differentiation includes at least: reported, reported but not yet investigated, under investigation, confirmed, solution not implemented yet and implemented." ;
    ids3cm:testCaseSpecification ids3c-co:TCS_ReportedSecurityFlaws, ids3c-co:TCS_SecurityFlawDescription .

_:n3-432 a ids3cm:FinalCondition ;
    rdfs:label "Final Condition for TCS_CorrectiveMeasures"@en ;
    dct:description "none"@en .

